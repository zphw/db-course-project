{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-labels": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "func-call-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "no-multiple-empty-lines": 2,
    "space-infix-ops": 2,
    "arrow-spacing": 2,
    "no-var": 2,
    "prefer-const": 2,
    "no-unsafe-negation": 2,
    "array-callback-return": 2,
    "dot-notation": 2,
    "no-eval": 2,
    "semi": 2,
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "no-restricted-imports": 2,
    "no-duplicate-imports": 2,
    "no-useless-computed-key": 2,
    "no-useless-rename": 2,
    "require-atomic-updates": 0,
    "no-new-func": 2,
    "require-await": 2,
    "no-unused-vars": 2
  }
}
